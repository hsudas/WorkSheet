@model IEnumerable<WorkSheet.Models.Entity.Menu>

@{
    Layout = null;
}

    

@foreach (var item in Model) {
    if (item.ParentId == null)
    {
    <li class="start active ">
        <a href="@item.Url">
            <i class="icon-home"></i>
            <span class="title">@Html.DisplayFor(modelItem => item.Name)</span>        
        
                @*@if (item.ParentId == null)*@
                @foreach (var item2 in Model.Where(x => x.ParentId == item.MenuId))
                {
                    @Html.DisplayFor(modelItem => item2.Name)
                    
                }
         </a>
     </li>
    }
}





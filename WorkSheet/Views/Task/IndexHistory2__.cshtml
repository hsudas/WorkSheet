    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/dataTables.bootstrap.min.js"></script>
    <script type="text/javascript" class="init">

        $(document).ready(function () {
            $('#example').DataTable();
        });

    </script>
    <title>IndexHistory2</title>
</head>
<body>

    <div>
        <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Description</th>
                    <th>Quantity</th>
                    <th>Minute</th>
                    <th>Phone</th>
                    <th>Date Added</th>
                </tr>
            </thead>
            <tbody>
                @{
                    foreach (var p in ViewBag.TaskList)
                    {
                        <tr>
                            <td>@p.Description</td>
                            <td>@p.Quantity</td>
                            <td>@p.Minute</td>
                            <td>@p.EventDate</td>
                            <td>@p.User.Name</td>
                        </tr>
                    }
                }
            </tbody>
            <tfoot>
                <tr>
                    <th>Name</th>
                    <th>Surname</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Date Added</th>
                </tr>
            </tfoot>
        </table>
        @*<div id="pager" style="position: none;">
